<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install PUBLIC "-//Joomla! 2.5//DTD template 1.0//EN" "http://www.joomla.org/xml/dtd/2.5/template-install.dtd">
<extension version="3.1" type="template" client="site" method="upgrade">
	<name>joomla-template-starter-kit</name>
	<version>1.0.0</version>
	<creationDate></creationDate>
	<author></author>
	<authorEmail></authorEmail>
    <authorUrl></authorUrl>
	<copyright>Copyright (C) 2005 - 2015 Open Source Matters, Inc. All rights reserved.</copyright>
	<description></description>
    
	<files>
		<folder>css</folder>
		<folder>html</folder>
		<folder>js</folder>
		<folder>scss</folder>
		<filename>README.md</filename>
		<filename>package.json</filename>
		<filename>gulpfile.js</filename>
		<filename>component.php</filename>
		<filename>error.php</filename>
		<filename>logic.php</filename>
		<filename>index.php</filename>
		<filename>favicon.ico</filename>
		<filename>templateDetails.xml</filename>
		<filename>template_preview.png</filename>
		<filename>template_thumbnail.png</filename>
	</files>
    
	<positions>
		<position></position>
	</positions>
    
	<config>
		<fields name="params">
			<fieldset name="advanced">
            
                <field name="googleFont"
					type="radio"
					class="btn-group btn-group-yesno"
					default="1"
					label="TPL_PROTOSTAR_FONT_LABEL"
					description="TPL_PROTOSTAR_FONT_DESC"
				>
					<option value="1">JYES</option>
					<option value="0">JNO</option>
				</field>
                
                <field name="googleFontName" class="" type="text" default="Open+Sans"
					label="Google Font Name"
					description="What's the name of the Font you want to include?" />
                
			</fieldset>
		</fields>
	</config>
</extension>
